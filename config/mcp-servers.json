{
  "servers": [
    {
      "name": "BioMCP Remote",
      "type": "streamable-http",
      "url": "https://remote.biomcp.org/mcp",
      "description": "Genomoncology BioMCP remote server with hosted OAuth sign-in",
      "auth": {
        "type": "oauth",
        "clientId": "biomcp-client",
        "authorizationEndpoint": "https://remote.biomcp.org/authorize",
        "tokenEndpoint": "https://remote.biomcp.org/token",
        "scopes": ["openid", "profile", "email"],
        "authorizationParams": {
          "access_type": "offline",
          "prompt": "select_account"
        }
      }
    },
